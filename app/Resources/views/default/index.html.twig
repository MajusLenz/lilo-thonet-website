                        <input type="text" class="js-range-slider real-input" name="Jahr" value="{{ auswahl["Jahr"]["min"] }};{{ auswahl["Jahr"]["max"] }}"
                               data-type="double"
                               data-min="{{ minDBJahr }}"
                               data-max="{{ maxDBJahr }}"
                               data-from="{{ auswahl["Jahr"]["min"] }}"
                               data-to="{{ auswahl["Jahr"]["max"] }}"
